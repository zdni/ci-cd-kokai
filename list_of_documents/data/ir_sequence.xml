<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_amendment_document" model="ir.sequence">
            <field name="name">Amendment Document</field>
            <field name="code">amendment.document</field>
            <field name="padding">4</field>
