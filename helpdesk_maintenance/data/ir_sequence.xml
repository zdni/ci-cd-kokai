<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_helpdesk_ticket" model="ir.sequence">
            <field name="name">Helpdesk Ticket</field>
            <field name="code">helpdesk.ticket</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">TICKET %(year)s/%(month)s/</field>
        </record>
    
        <record id="seq_helpdesk_activity" model="ir.sequence">
            <field name="name">Helpdesk Activity</field>
            <field name="code">helpdesk.activity</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">TICKET-ACT %(year)s/%(month)s</field>
        </record>

    </data>

</odoo>
