<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_annual_training" model="ir.sequence">
            <field name="name">Annual Training</field>
            <field name="code">annual.training</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">ANN-TRAIN %(year)s/</field>
        </record>
    
        <record id="seq_hr_training" model="ir.sequence">
            <field name="name">HR Training</field>
            <field name="code">hr.training</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">TRAINING %(year)s/%(month)s/</field>
        </record>
    
    </data>

</odoo>
