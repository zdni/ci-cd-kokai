<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_approval_inspection" model="ir.sequence">
            <field name="name">Approval Inspection</field>
            <field name="code">approval.inspection</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="suffix">/APP-INS/%(year)s/%(month)s</field>
        </record>
    
        <record id="seq_component_inspection" model="ir.sequence">
            <field name="name">Component Inspection</field>
            <field name="code">component.inspection</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="suffix">/COMP-INS/%(year)s/%(month)s</field>
        </record>
    
    </data>

</odoo>
