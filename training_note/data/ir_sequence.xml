<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_training_note" model="ir.sequence">
            <field name="name">Training Note</field>
            <field name="code">training.note</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">TRAIN-NOTE %(year)s/%(month)s/</field>
        </record>
    
    </data>

</odoo>
