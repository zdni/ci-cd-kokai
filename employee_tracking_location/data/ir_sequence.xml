<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_employee_tracking_location" model="ir.sequence">
            <field name="name">Tracking Location</field>
            <field name="code">employee.tracking.location</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">daily</field>
            <field name="prefix">TRACK-LOC/%(year)s/%(month)s/%(day)s/</field>
        </record>
    
    </data>

</odoo>
