<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_customer_complaint" model="ir.sequence">
            <field name="name">Customer Complaint</field>
            <field name="code">customer.complaint</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">yearly</field>
            <field name="prefix">CC/%(year)s/</field>
        </record>
    
        <record id="seq_line_complaint" model="ir.sequence">
            <field name="name">Line Complaint</field>
            <field name="code">line.complaint</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">yearly</field>
            <field name="prefix">COMPLAINT/%(year)s/</field>
        </record>

    </data>

</odoo>
