<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="seq_schedule_task" model="ir.sequence">
            <field name="name">Schedule Task</field>
            <field name="code">schedule.task</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">Task %(year)s/%(month)s/</field>
        </record>
    
        <record id="seq_notification_task" model="ir.sequence">
            <field name="name">Notification Task</field>
            <field name="code">notification.task</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">daily</field>
            <field name="prefix">NOTIF/%(year)s/%(month)s/%(day)s/</field>
        </record>
    
        <record id="seq_assignment_task" model="ir.sequence">
            <field name="name">Assignment Task</field>
            <field name="code">assignment.task</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">monthly</field>
            <field name="prefix">ASS-TASK/%(year)s/%(month)s/</field>
        </record>
    
    </data>

</odoo>
