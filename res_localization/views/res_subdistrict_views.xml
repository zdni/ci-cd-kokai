<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="res_subdistrict_view_tree" model="ir.ui.view">
            <field name="name">res.subdistrict.view.tree</field>
            <field name="model">res.subdistrict</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Subdistrict">
                    <field name="name" />
                    <field name="city_id" />
                </tree>
            </field>
        </record>

        <record id="res_subdistrict_action" model="ir.actions.act_window">
            <field name="name">Subdistrict</field>
            <field name="res_model">res.subdistrict</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            id="res_subdistrict_menu"
            name="Subdistrict"
            action="res_subdistrict_action"
            parent="res_localization_menu"
            sequence="10"
        />
    
    </data>
    

</odoo>
